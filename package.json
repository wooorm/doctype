{
  "name": "doctype",
  "version": "2.0.0",
  "description": "Information for HTML doctypes",
  "license": "MIT",
  "keywords": [
    "xhtml",
    "html",
    "svg",
    "mathml",
    "doctype",
    "doctypes",
    "info",
    "information"
  ],
  "repository": "https://github.com/wooorm/doctype",
  "bugs": "https://github.com/wooorm/doctype/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js",
    "index.json"
  ],
  "dependencies": {},
  "devDependencies": {
    "browserify": "^14.0.0",
    "esmangle": "^1.0.0",
    "nyc": "^10.0.0",
    "remark-cli": "^2.0.0",
    "remark-preset-wooorm": "^2.0.0",
    "tape": "^4.4.0",
    "xo": "^0.17.0"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail",
    "build-bundle": "browserify index.js -s doctype > doctype.js",
    "build-mangle": "esmangle doctype.js > doctype.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "rules": {
      "guard-for-in": "off"
    },
    "ignores": [
      "doctype.js"
    ]
  },
  "remarkConfig": {
    "output": true,
    "presets": "wooorm"
  }
}
